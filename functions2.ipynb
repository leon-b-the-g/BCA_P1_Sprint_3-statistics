    "my_list = [1,2,3,5,7,6]\n",
    "\n",
    "def find_even(a_list):\n",
    "    evens = []\n",
    "    for element in a_list:\n",
    "        if element % 2 == 0:\n",
    "            evens.append(element)\n",
    "        else:\n",
    "            continue\n",
    "    return evens\n",
    "\n",
    "my_even_nums = find_even(my_list)\n",
    "\n",
    "print(my_even_nums)"
   ]
